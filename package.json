{
  "name": "uab-institute-management",
  "version": "1.0.0",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/src/server.ts",
    "client": "cd client && npm start",
    "build-client": "cd client && npm install && npm run build",
    "build-server": "cd server && npm install --production=false && npm run build",
    "build": "npm run build-client && npm run build-server",
    "start": "node server/dist/server.js",
    "seed": "cd server && npm run db:seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "institute-management",
    "education"
  ],
  "author": "",
  "license": "ISC",
  "description": "UAB Institute Management System with React",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.16.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.9"
  }
}