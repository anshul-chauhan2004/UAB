{
    "name": "uab-lms-server",
    "version": "1.0.0",
    "description": "Real-time Learning Management System Backend",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
